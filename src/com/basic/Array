package com.basic;

public class Array {
	public static void main (String args[]) {
		int i=0;
		int N = 78945;
		int t =N;
		int array[] = new int[10];
       while(t > 0) {
    	   int remainder = N % 10;
    	   array [i] = remainder;
    	   i++;
    	   t = t/10;
    	   
    	   
		}
       for(i=0;i<10;i++)
    	   System.out.println(array[i]);
			
	}

}
