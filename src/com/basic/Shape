package com.basic;

public class Shape {
	//double side;
	double length;
	double breadth;
		public Shape(double length , double breadth) {
			if (length > 0.0 && length < 20.0 && breadth > 0.0 && breadth < 20.0) {
			this.length = length;
			this.breadth = breadth;
			
		}else
		{
			System.out.println("length and breath between with in the limit");
		}
	}public double FindAreaofRectangle() {
		return length*breadth;
	}
 public static void main(String []args) {
	 Shape rectangle = new Shape(5,19.9);
	 rectangle.FindAreaofRectangle();
	 System.out.println(rectangle.FindAreaofRectangle());
	 
 }
}
